pipeline {
    agent any
    stages {
        stage ("build") {
            steps {
                echo "Jenkins делает сборку"
            }
        }

        stage ("test") {
            steps {
                echo "Jenkins делает тесты"
            }
        }

        stage ("deploy") {
            steps {
                echo "Jenkins делает загрузку"
            }
        }
    }
}